{
  "version": 2,
  "name": "430-backend",
  "builds": [
      {
          "src": "dist/main.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "dist/main.js",
          "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
          ]
      }
  ]
}